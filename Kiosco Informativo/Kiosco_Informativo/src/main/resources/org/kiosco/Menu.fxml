<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="TOP_CENTER" prefHeight="650.0" prefWidth="250.0" stylesheets="@Tema.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.kiosco.controllers.MenuController">
   <children>
      <ImageView id="imvLogo" fitHeight="160.0" fitWidth="240.0" pickOnBounds="true" preserveRatio="true">
         <VBox.margin>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </VBox.margin>
      </ImageView>
      <Button contentDisplay="TOP" mnemonicParsing="false" onAction="#actTextos" prefHeight="65.0" prefWidth="240.0" styleClass="boton" text="Textos">
         <VBox.margin>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </VBox.margin>
         <graphic>
            <ImageView id="imvTextos" fitHeight="30.0" fitWidth="30.0" pickOnBounds="true" preserveRatio="true" />
         </graphic>
      </Button>
      <Button contentDisplay="TOP" layoutX="10.0" layoutY="160.0" mnemonicParsing="false" onAction="#actImagenes" prefHeight="65.0" prefWidth="240.0" styleClass="boton" text="Imagenes">
         <VBox.margin>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </VBox.margin>
         <graphic>
            <ImageView id="imvImagenes" fitHeight="30.0" fitWidth="30.0" pickOnBounds="true" preserveRatio="true" />
         </graphic>
      </Button>
      <Button contentDisplay="TOP" layoutX="10.0" layoutY="185.0" mnemonicParsing="false" onAction="#actVideos" prefHeight="65.0" prefWidth="240.0" styleClass="boton" text="Videos">
         <VBox.margin>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </VBox.margin>
         <graphic>
            <ImageView id="imvVideos" fitHeight="30.0" fitWidth="30.0" pickOnBounds="true" preserveRatio="true" />
         </graphic>
      </Button>
      <Button contentDisplay="TOP" layoutX="10.0" layoutY="210.0" mnemonicParsing="false" onAction="#actEnlaces" prefHeight="65.0" prefWidth="240.0" styleClass="boton" text="Youtube">
         <VBox.margin>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </VBox.margin>
         <graphic>
            <ImageView id="imvYoutube" fitHeight="30.0" fitWidth="30.0" pickOnBounds="true" preserveRatio="true" />
         </graphic>
      </Button>
      <Button contentDisplay="TOP" layoutX="10.0" layoutY="235.0" mnemonicParsing="false" onAction="#actPresentar" prefHeight="65.0" prefWidth="240.0" styleClass="boton" text="Presentar">
         <VBox.margin>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </VBox.margin>
         <graphic>
            <ImageView id="imvPresentar" fitHeight="30.0" fitWidth="30.0" pickOnBounds="true" preserveRatio="true" />
         </graphic>
      </Button>
   </children>
</VBox>
